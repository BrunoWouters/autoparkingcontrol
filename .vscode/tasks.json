{
    "version": "2.0.0",
    "tasks": [
		{
			"label": "dapr",
			"type": "dapr",
			"runFile": "./dapr.yaml",
			"dependsOn": [
				"build"
			],
			"problemMatcher": [
				"$tsc"
			],
		},
		{
			"type": "dotnet",
			"task": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "dotnet: build"
		}
	]
}
