{
    "version": "2.0.0",
    "tasks": [	
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/AutoParkingControl.sln"
			],
			"group":{
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": "$msCompile"
		},		
		{
			"label": "dapr",
			"type": "dapr",
			"runFile": "./dapr.yaml",
			"dependsOn": [
				"build"
			]
		}
    ]
}
